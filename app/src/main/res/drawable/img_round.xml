<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval" >
            <size
                android:width="94dp"
                android:height="94dp"/>
            <gradient
                android:angle="45"
                android:startColor="#FECD00"
                android:endColor="#C913B9"
                android:type="linear" />
            <corners
                android:radius="100dp"/>
        </shape>
    </item>

    <item
        android:bottom="2.5dp"
        android:left="2.5dp"
        android:right="2.5dp"
        android:top="2.5dp">
        <shape android:shape="oval" >
            <corners
                android:radius="100dp"/>
            <solid
                android:color="#fff" />
        </shape>
    </item>

</layer-list>